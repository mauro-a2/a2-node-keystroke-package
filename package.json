{
  "name": "a2-node-keystroke-package",
  "version": "0.0.0",
  "type": "module",
  "author": "Mauro Ruiz",
  "module": "dist/a2-node-keystroke-package.esm.js",
  "keywords": [
    "node",
    "keystroke",
    "keyboard",
    "area2",
    "a2"
  ],
  "license": "MIT",
  "homepage": "https://docs.area2-ai.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mauro-a2/a2-node-keystroke-package.git"
  },
  "files": [
    "dist/"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=10"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "push:npm": "np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/a2-node-keystroke-package.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/a2-node-keystroke-package.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "husky": "^9.1.7",
    "np": "^10.1.0",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1"
  },
  "dependencies": {
    "emoji-regex": "^10.4.0",
    "uuid": "^11.0.4"
  }
